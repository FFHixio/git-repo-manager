{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "max-line-length": [
          210
        ],
        "quotemark": [
          false
        ],
        "no-trailing-whitespace": [
          false
        ]
    }
}
