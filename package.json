{
  "name": "GitHubControl",
  "version": "0.0.0",
  "description": "This application tracks versions of package.json files dependencies at github repositories",
  "author": "Nikita Glukhi <nikita.glukhi@valor-software.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:frontend": "cd front-end && ng serve",
    "start:server": "cd back-end && ts-node -r tsconfig-paths/register src/main.ts",
    "build": "ng build",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@nestjs/common": "5.4.0",
    "@nestjs/core": "5.4.0",
    "@nestjs/mongoose": "5.2.2",
    "@nestjs/typeorm": "2.1.0",
    "@nestjs/websockets": "5.4.0",
    "@types/lodash": "4.14.118",
    "compare-versions": "3.4.0",
    "core-js": "2.5.7",
    "cors": "2.8.4",
    "hbs": "4.0.1",
    "lodash": "4.17.11",
    "mongoose": "5.3.12",
    "prettier": "1.15.2",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "rxjs": "6.3.3",
    "semver": "5.6.0",
    "typeorm": "0.2.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.7",
    "@angular/cli": "6.2.4",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@nestjs/testing": "5.4.0",
    "@types/express": "4.16.0",
    "@types/jasmine": "2.8.12",
    "@types/jasminewd2": "2.0.6",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.118",
    "@types/mongoose": "5.3.1",
    "@types/node": "10.12.9",
    "@types/supertest": "2.0.6",
    "codelyzer": "4.3.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.6.0",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "nodemon": "1.18.6",
    "prettier": "1.15.2",
    "protractor": "5.4.1",
    "rimraf": "2.6.2",
    "supertest": "3.3.0",
    "ts-jest": "23.10.4",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.26.0",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
